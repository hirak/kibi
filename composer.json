{
    "name": "hirak/kibi",
    "description": "Full auto Dependency Injection Container for PHP7",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Hiraku NAKANO",
            "email": "hiraku@tojiru.net"
        }
    ],
    "require": {
        "php": "^7.0",
        "composer-plugin-api": "^1.1",
        "container-interop/container-interop": "^1.1"
    },
    "provide": {
        "container-interop/container-interop-implementation": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Hirak\\Kibi\\": "src"
        }
    },
    "extra": {
        "class": "Hirak\\Kibi\\Plugin"
    }
}
